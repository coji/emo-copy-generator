{
  "name": "emo-copy-generator",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "kysely:codegen": "kysely-codegen --dialect libsql --singularize --camel-case --out-file app/services/types.ts --url file:./local.db",
    "db:migrate": "tsx --env-file .env scripts/migrate.ts",
    "db:seed": "tsx --env-file .env scripts/seed-templates.ts",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier . --check --experimental-cli",
    "format:fix": "prettier . --write --experimental-cli",
    "lint": "biome lint .",
    "validate": "run-p format lint typecheck"
  },
  "dependencies": {
    "@ai-sdk/google": "2.0.3",
    "@ai-sdk/openai": "2.0.5",
    "@ai-sdk/react": "2.0.8",
    "@conform-to/react": "1.8.2",
    "@conform-to/zod": "1.8.2",
    "@libsql/client": "0.15.10",
    "@libsql/kysely-libsql": "0.4.1",
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-visually-hidden": "1.2.3",
    "@react-router/express": "7.8.0",
    "@react-router/node": "7.8.0",
    "@react-router/serve": "^7.8.0",
    "@tabler/icons-react": "3.34.1",
    "@vercel/react-router": "1.2.2",
    "ai": "5.0.8",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "isbot": "^5.1.29",
    "kysely": "0.28.4",
    "lucide-react": "0.537.0",
    "next-themes": "0.4.6",
    "radix-ui": "latest",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "10.1.0",
    "react-router": "^7.8.0",
    "react-twc": "1.5.1",
    "remix-toast": "3.1.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "ts-pattern": "5.8.0",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@react-router/dev": "^7.8.0",
    "@tailwindcss/vite": "4.1.11",
    "@types/express": "5.0.3",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "12.2.0",
    "cross-env": "^10.0.0",
    "kysely-codegen": "0.18.5",
    "kysely-ctl": "0.14.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "1.3.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.0",
    "vite-plugin-devtools-json": "0.4.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "workerd"
    ]
  }
}
