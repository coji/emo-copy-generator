{
  "name": "emo-copy-generator",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "kysely:codegen": "kysely-codegen --dialect libsql --singularize --camel-case --out-file app/services/types.ts --url file:./local.db",
    "db:migrate": "tsx --env-file .env scripts/migrate.ts",
    "db:seed": "tsx --env-file .env scripts/seed-templates.ts",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier . --check --experimental-cli",
    "format:fix": "prettier . --write --experimental-cli",
    "lint": "biome lint .",
    "validate": "run-p format lint typecheck"
  },
  "dependencies": {
    "@ai-sdk/google": "2.0.16",
    "@ai-sdk/openai": "2.0.35",
    "@ai-sdk/provider": "2.0.0",
    "@ai-sdk/react": "2.0.52",
    "@conform-to/react": "1.10.1",
    "@conform-to/zod": "1.10.1",
    "@libsql/client": "0.15.15",
    "@libsql/kysely-libsql": "0.4.1",
    "@paralleldrive/cuid2": "2.2.2",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-visually-hidden": "1.2.3",
    "@react-router/express": "7.9.2",
    "@react-router/node": "7.9.2",
    "@react-router/serve": "^7.9.2",
    "@tabler/icons-react": "3.35.0",
    "@types/dompurify": "3.2.0",
    "@vercel/react-router": "1.2.3",
    "ai": "5.0.52",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "dotenv": "17.2.2",
    "express": "5.1.0",
    "isbot": "^5.1.31",
    "isomorphic-dompurify": "2.28.0",
    "kysely": "0.28.7",
    "lucide-react": "0.544.0",
    "next-themes": "0.4.6",
    "radix-ui": "latest",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "10.1.0",
    "react-router": "^7.9.2",
    "react-twc": "1.5.1",
    "remix-toast": "3.3.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "tokenlens": "1.3.1",
    "ts-pattern": "5.8.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@react-router/dev": "^7.9.2",
    "@tailwindcss/vite": "4.1.13",
    "@types/express": "5.0.3",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "12.4.1",
    "cross-env": "^10.0.0",
    "kysely-codegen": "0.19.0",
    "kysely-ctl": "0.19.0",
    "npm-run-all": "4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "1.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-devtools-json": "1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "workerd"
    ]
  }
}
